# Example terraform.tfvars (do NOT store secrets in version control)
# Copy this to terraform.tfvars and fill in values locally.

project_id = "REPLACE_WITH_PROJECT_ID"
region     = "asia-east1"

images_bucket = "REPLACE_WITH_UNIQUE_IMAGES_BUCKET"
logs_bucket = "REPLACE_WITH_UNIQUE_LOGS_BUCKET"

# Path to Firebase admin service account JSON (local only, not committed)
firebase_admin_key_file = "~/firebase-admin-sa-key.json"
